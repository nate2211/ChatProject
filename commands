python main.py pipeline "Raf Simons (Belgian fashion designer)" --extra pipeline.stages="guard|tools|memory|tensor|webcorpus|corpus|chat|self_check" --extra tensor.task="zero-shot-classification" --extra tensor.labels="fashion,networking,audio processing,systems programming,cryptocurrency" --extra tensor.inject_format="simple" --extra tensor.output_key="prompt_topic" --extra tensor.return_all_scores=true --extra tensor.score_threshold=0.0 --extra memory.op=clear --extra memory.key=raf_lex --extra webcorpus.query="Raf Simons (Belgian fashion designer)" --extra webcorpus.engine=duckduckgo --extra webcorpus.num_results=25 --extra webcorpus.max_fetch=20 --extra webcorpus.sent_window=2 --extra webcorpus.top_k_docs=12 --extra webcorpus.top_k_sents=40 --extra webcorpus.min_doc_chars=200 --extra webcorpus.site_include="wikipedia.org,vogue.com,businessoffashion.com,gq.com,interviewmagazine.com,prada.com,dior.com,calvinklein.us,rafsimons.com,metmuseum.org,showstudio.com" --extra webcorpus.inject_lexicon=true --extra webcorpus.inject_context=true --extra webcorpus.export_context=true --extra webcorpus.context_key=raf_ctx --extra corpus.query="Raf Simons (Belgian fashion designer)" --extra corpus.provider=wiki_api --extra corpus.sample=120 --extra corpus.top_k_docs=12 --extra corpus.top_k_sents=40 --extra corpus.sent_window=2 --extra corpus.min_doc_chars=200 --extra corpus.inject_lexicon=true --extra corpus.inject_context=true --extra corpus.lexicon_key=raf_lex --extra corpus.must_terms="designer" --extra corpus.neg="Royal Air Force,aircraft,Airbus,infrared,Simons Island,Simon Says" --extra chat.model=lexicon --extra chat.style=outline --extra chat.max_terms=25 --extra chat.max_chars=2400 --extra chat.lexicon_key=raf_lex --extra chat.keep_history=false --extra chat.include_context=true